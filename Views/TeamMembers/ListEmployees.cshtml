@model Sitecore.Data.Items.Item
@using Sitecore.Data.Fields
@using Sitecore.Data.Items
@using Sitecore.Resources.Media
@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation

<div class="container">
    <div class="row">
        <div class="col-lg-12 text-center">
            <h2 class="section-heading">@Html.Sitecore().Field("Title")</h2>
            <h3 class="section-subheading text-muted">@Html.Sitecore().Field("Subtitle")</h3>
        </div>
    </div>
    <div class="row">
        @Html.Sitecore().Placeholder("teammates")
    </div>
    <div class="row">
        <div class="col-lg-8 col-lg-offset-2 text-center">
            <p class="large text-muted">@Html.Sitecore().Field("Content")</p>
        </div>
    </div>
</div>
